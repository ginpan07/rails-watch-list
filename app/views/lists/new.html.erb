<!-- app/views/restaurants/edit.html.erb -->

<%= form_with model: @list do |f| %>
  <div class="mb-3">
    <%= f.label :name,  class: "form-label" %>
    <%= f.text_field :name, class: "form-control w-50"%>
  </div>
  <div class="mb-3">
    <%= f.submit "Save",  class: "btn btn-primary" %>
  </div>
<% end %>
